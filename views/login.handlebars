<form id="login-form" class="mt-5 mb-5">
  <div class="mb-3">
    <label for="email-login" class="form-label">Email Address</label>
    <input
      type="text"
      id="email-login"
      class="form-control"
      aria-describedby="emailHelp"
    />
  </div>
  <div class="mb-3">
    <label for="password-login" class="form-label">Password</label>
    <input type="password" id="password-login" class="form-control" />
  </div>
  <div>
    <button class="btn btn-outline-dark" type="submit">Login</button>
  </div>
</form>
<script src="/js/login.js"></script>

<div>
  <p class="fs-4 font-monospace text-center">Oops! Looks like something went
    terribly wrong... Go ahead a try again!</p>
</div>